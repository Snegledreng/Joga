@page
@using Joga.model;
@using Joga.services;
@model Joga.Pages.Admin.MedlemslisteModel
@{
}
<table class="table table-hover">
    <thead>
        <tr>
            <th>Id</th>
            <th>Navn</th>
            <th>Email</th>
            <th>Telefon</th>
            <th>Aktivt Medlem</th>
            <th>Nyhedsbrev</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (Medlem m in Model.Medlemmer)
        {
            <tr>
                <td>@m.Id</td>
                <td>@m.Navn</td>
                <td>@m.Email</td>
                <td>@m.Telefon</td>
                <td>@m.Status</td>
                <td>@m.Nyhedsbrev</td>
            </tr>
        }
    </tbody>
</table>